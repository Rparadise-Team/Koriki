'SWAGGY GAME BY BLOCKHEAD

RANDOMIZE TIMER
TOUCHSCREEN

GLOBAL SPIKES,DEATH,FARMER,FARMERTIME,FARMERHIT,SCORE,SCORED,LEVEL
DIM GLOBAL HITTED(10),SPIKEON(10),TIME(10)

MUSIC

SCREEN:
SPRITE OFF
CLS 0
BG 1
BG COPY 0,0,20,16 TO 0,0
SPRITE.A 0,(,,0,,)
FONT 64
SPRITE 0,-20,82,1
SPRITE.A 0,(,,,,1)
TICK=0
SCROLL 1,0,0

WAIT VBL

DO
SPRITE 0,SPRITE.X(0)+1,,
TICK=TICK+1
IF TICK>=15 THEN 
 SPRITE 0,,,((TIMER MOD 2)*2)+1
 TICK=0
END IF
 IF TIMER MOD 30<20 THEN
  TEXT 4,9,"TAP TO START!"
 ELSE
  TEXT 4,9,"             "
 END IF
IF TAP THEN GOTO READY
WAIT VBL
LOOP

READY:
ATTR (,,,1)
BG 0
BG FILL 0,15 TO 31,31 CHAR 15

FOR I=0 TO 129
SCROLL 0,SCROLL.X(0),SCROLL.Y(0)-1
TICK=TICK+1
SPRITE 0,SPRITE.X(0)+1,,
IF TICK>=15 THEN 
 SPRITE 0,,,((TIMER MOD 2)*2)+1
 TICK=0
END IF
WAIT VBL
NEXT I

BG 1
CLS 1
ATTR (,,,0)
BG FILL 0,12 TO 31,31 CHAR 7
BG FILL 0,13 TO 31,31 CHAR 0
SPRITE 0,15,82,1
BG FILL 0,20 TO 5,20 CHAR 0

FOR I=1 TO 4
CELL INT(RND*32),11,(INT(RND*2)+8)
NEXT I

FOR I=1 TO 12
CELL INT(RND*32),INT(RND*6),13
NEXT I

BG 0

FOR I=0 TO 125
SCROLL 0,SCROLL.X(0),SCROLL.Y(0)-1
WAIT VBL
NEXT I
CLS 0
SCROLL 0,0,0

GAME:
FARMERTIME=300
SCORE=0
LEVEL=0
SPRITE 0,15,82,1
Y=SPRITE.Y(0)
JUMP=0
ONGROUND=1
SPIKES=1
FOR I=0 TO 5
TIME(I)=100+(I*INT(RND*10))
NEXT I
DEATH=0
FALLED=0

DO
IF LEVEL>=2 THEN
SPIKES=SPIKES+1
LEVEL=0
END IF
SPRITE 0,,Y,
SCROLL 1,SCROLL.X(1)+2.5,SCROLL.Y(1)
TICK=TICK+1
IF TICK>=15 THEN 
 SPRITE 0,,,((TIMER MOD 2)*2)+1
 TICK=0
END IF
IF TAP AND ONGROUND=1 AND JUMP=0 AND DEATH=0 THEN
 ONGROUND=0
 JUMP=1
END IF
IF Y>65 AND JUMP=1 AND DEATH=0 THEN
 Y=Y-1
ELSE IF Y<=65 THEN
 JUMP=0
END IF
IF JUMP=0 AND ONGROUND=0 AND NOT Y>=82 THEN 
 Y=Y+1
ELSE IF Y>=82 THEN
 ONGROUND=1
END IF
WAIT VBL
CELL 0,0,23
NUMBER 1,0,SCORE,5
CALL SPIKE
IF SPRITE HIT (0,1 TO 11) THEN
DEATH=1
SPRITE.A 0,(,,1,,)
END IF
IF FALLED=0 AND DEATH=1 AND Y<180 THEN
Y=Y+1
ELSE IF Y>=180 THEN
FALLED=1
END IF
IF FALLED=1 AND DEATH=1 THEN
IF TIMER MOD 30<20 THEN
  TEXT 2,9,"TAP TO RESTART!"
 ELSE
  TEXT 2,9,"               "
 END IF
 IF TAP THEN GOTO NOPE
END IF
CALL FARMER
LOOP

SUB SPIKE
FOR I=0 TO SPIKES
 IF SPIKEON(I)=0 AND TIME(I)=0 AND DEATH=0 THEN
  SPIKEON(I)=1
  SPRITE I+1,160,88,12
 ELSE IF TIME(I)>0 THEN
  TIME(I)=TIME(I)-1
 ELSE IF SPIKEON(I)=1 AND SPRITE.X(I+1)>-10 THEN
  SPRITE I+1,SPRITE.X(I+1)-1.5,,
 ELSE IF SPIKEON(I)=1 AND SPRITE.X(I+1)<=-10 THEN
  SPRITE OFF I+1
  SPIKEON(I)=0
  TIME(I)=180
 END IF
NEXT I
END SUB

NOPE:
ATTR (,,,1)
CLS 0
BG 0
BG FILL 0,15 TO 31,31 CHAR 15

FOR I=0 TO 129
SCROLL 0,SCROLL.X(0),SCROLL.Y(0)-1
SCROLL 1,SCROLL.X(1)+2.5,SCROLL.Y(1)
WAIT VBL
NEXT I

BG COPY 0,0,20,16 TO 0,0
CLS 1
SPRITE OFF

FOR I=0 TO 125
SCROLL 0,SCROLL.X(0),SCROLL.Y(0)-1
WAIT VBL
NEXT I
CLS 0
SCROLL 0,0,0
GOTO SCREEN

SUB FARMER
IF FARMERTIME<1 AND FARMER=0 AND DEATH=0 AND FARMERHIT=0 THEN
SPRITE 12,180,82,5
FARMER=1
SPRITE.A 12,(,,0,,1)
ELSE IF FARMERTIME>0 THEN
FARMERTIME=FARMERTIME-1
ELSE IF FARMER=1 AND SPRITE.X(12)>-15 AND FARMERHIT=0 THEN
SPRITE 12,SPRITE.X(12)-1.5,,
ELSE IF SPRITE.X(12)<=-15 THEN
SPRITE OFF 12
FARMER=0
FARMERTIME=300
END IF
IF SPRITE HIT (0,12) AND DEATH=0 THEN
FARMERHIT=1
SPRITE.A 12,(,,1,,)
IF SCORED=0 THEN
SCORE=SCORE+1
LEVEL=LEVEL+1
SCORED=1
END IF
END IF
IF FARMERHIT=1 AND SPRITE.Y(12)<180 THEN
SPRITE 12,,SPRITE.Y(12)+1,
ELSE IF SPRITE.Y(12)>=180 THEN
SPRITE OFF 12
SPRITE.A 12,(,,0,,)
FARMER=0
FARMERHIT=0
FARMERTIME=700
SCORED=0
END IF
END SUB

