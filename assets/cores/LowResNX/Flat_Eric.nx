RANDOMIZE TIMER
FONT 64
ATTR(1)
SPRITE.A 0,(3,0,0,0,3)
SPRITE.A 1,(4,0,0,0,3)

T$=" FLAT ERIC FLAT ERIC "
X=80
Y=60

CURCY=0
R=1
S=2
YS=1

MUSIC

DO
  BG 1
  M$=MID$(T$,1,20)
  TEXT 0,YS,M$
  L$=LEFT$(T$,1)
  R$=RIGHT$(T$,LEN(T$)-1)
  T$=R$+L$
  IF INT(RND*10)=1 THEN 
     R=INT(RND*4)+1
     YS=INT(RND*16)
  END IF
  IF R=1 AND X<127 THEN X=X+S
  IF R=2 AND X>1 THEN  X=X-S
  IF R=3 AND Y<95 THEN Y=Y+S
  IF R=4 AND Y>1 THEN Y=Y-S
  SPRITE 0,X,Y,1
  SPRITE 1,X+20,Y+10,12
  BG 0
  REM TIMOS CODE***********************
  SY=SY-1
  IF SY<0 THEN SY=SY+256
  CY=SY\8
  IF CY<>CURCY THEN
     FOR CX=0 TO 31
         CH=5+INT(RND*7)
         CELL CX,CY,CH
     NEXT CX
  CURCY=CY
  END IF
  SCROLL 0,0,SY
  WAIT VBL
LOOP

