REM MUSIC BY DESBYC
RANDOMIZE TIMER
REM WASHAW GAMES
DIM X(3),Y(3),P(3),FACT(3)
DIM FL(3),FH(3),FR(3),FX(3)

FOR I=0 TO 3
    X(I)=71
    Y(I)=55
    SPRITE I+60,X(I),Y(I),2
    SPRITE.A I+60,(I,0,0,1,1)
NEXT I

FOR I=1 TO 9
    SPRITE I,200,200,32
    SPRITE.A I,(4,,,,1)
NEXT I
FOR I=11 TO 19
    SPRITE I,200,200,32
    SPRITE.A I,(5,,,,1)
NEXT I
FOR I=21 TO 29
    SPRITE I,200,200,32
    SPRITE.A I,(6,,,,1)
NEXT I
FOR I=31 TO 39
    SPRITE I,200,200,32
    SPRITE.A I,(7,,,,1)
NEXT I


DISPLAY (1,0,1,0,0)

MUSIC

REM START TWEEKS...
REM
REM 1
FX(0)=2
FX(1)=2
FX(2)=2
FX(3)=2
REM
REM 51
FACT(0)=30
FACT(1)=30
FACT(2)=30
FACT(3)=30
REM
REM END TWEEKS

HIP=0

DO

P(0)=PEEK($FF43)
P(1)=PEEK($FF4F)
P(2)=PEEK($FF5B)
P(3)=PEEK($FF67)

FOR I=0 TO 3
    IF P(I)>0 THEN
       ADDR=$FF40+(12*I)
       FL(I)=PEEK(ADDR)
       FH(I)=PEEK(ADDR+1)
       FR(I)=((FH(I)*256)+FL(I)-261)/300
       R=8+FR(I)*FX(I)
       SPR=P(I)\FACT(I)
       IF P(I)>0 AND SPR=0 THEN SPR=1
       IF SPR>5 THEN SPR=5
       SPRCHAR=32+(2*SPR)
       FOR II=(I*10+1) TO (I*10+9)
           A=RND*PI*2
           SPRITE II,X(I)+R*COS(A),Y(I)+R*SIN(A),SPRCHAR
       NEXT II
    ELSE
          FOR II=I*10+1 TO I*10+9
              SPRITE II,200,200,
          NEXT II
    END IF
NEXT I

LOOP

