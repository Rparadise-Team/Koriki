  INC T
UNTIL T=120
MUSIC SPARAS(SS,1)
DO
  CLS 0
  
  CALL DRAWHUD
  CALL DRAWBG
  CALL DRAWPLAYER
  
  CALL UPDATEHANDLER
  CALL UPDATEBULLETS
  CALL UPDATELASERS
  CALL UPDATEFIGHTERS
  CALL UPDATEPLAYER
  
