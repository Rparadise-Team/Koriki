ACD=MAXACD
PTRUE=0
CPSIZE=0
CPINV=0
CFINV=0

'LASER VARS
FOR I=0 TO 31
  FOR J=0 TO 3
    LASERS(I,J)=0
  NEXT J
NEXT I

'FIGHTER VARS
FOR I=0 TO MAXFIGHTERS-1
  FOR J=0 TO 6
    FIGHTERS(I,J)=0
  NEXT J
NEXT I

'BULLET VARS
FOR I=0 TO MAXBULLETS-1
  FOR J=0 TO 5
    BULLETS(I,J)=0
  NEXT J
NEXT I

'PLAYER VARS
PX=(AW*8)/2
PY=(AH*8)/2
PCHARGED=0

GAME:
WAIT VBL
BG 0
CALL DRAWHUD
CALL DRAWBG
PRIO 1
CALL CWTEXT("READY",0,8,AW)
PRIO 0
T=0
REPEAT
  CALL DRAWBG
