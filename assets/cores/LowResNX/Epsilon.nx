      PLAY 3,49,20 SOUND 1
      WAIT VBL
    END IF
  END IF
  IF TMODE=1 THEN
    ACTION=0
    
    IF CSTYLE=0 THEN
      IF UP TAP(0) THEN
        ACTION=1
      ELSE IF DOWN TAP(0) THEN
        ACTION=2
      END IF
      
      IF BUTTON TAP(0,0) THEN ACTION=3
    ELSE
      IF TAP THEN
        IF TOUCH.X>80 THEN
          ACTION=3
        ELSE
          IF TOUCH.Y>64 THEN ACTION=2 ELSE ACTION=1
        END IF
      END IF
    END IF
      
    IF ACTION=1 THEN
      DEC SS
      IF SS<0 THEN SS=0 ELSE PLAY 3,49,20 SOUND 1
    ELSE IF ACTION=2 THEN
      INC SS
      IF SS>MAXSS THEN SS=MAXSS ELSE PLAY 3,49,20 SOUND 1
    ELSE IF ACTION=3 THEN
      CLS
      SPRITE OFF 0 TO 63
      STOP
      POKEL $E004+(8*SS),PEEKL($E004+(8*SS))+1
      PLAY 3,49,20 SOUND 1
      DIFF=SS
      GOTO GAMESET
    END IF
  END IF
  
  SCROLL 1,SIN(TIMER/50)*5,-(TIMER/5)
  SPRITE 0,120+(SIN((TIMER-2)/50)*2),64+(COS((TIMER-2)/50)*2),1
  WAIT VBL
LOOP

