DIM COORDINATES(2,2),FLOOR_COLLIDE(2),JUMP(2,2),PUNCH(2),IMPULSE(2),CHARA(2,2),FLIPED(2)
DIM WALKTICK(2),BATTACK(2,2),DEFECT(2,3,2),BTICK(2),DEFCOOR(8,2),MISILE(2,2),SCORE(2)
DIM MODE(2)
DIM STAGE$(7),STAGEBG(7)
PAUSE OFF
RANDOMIZE TIMER
TIME=0
FONT 192
S=0
MODE(0)=0
MODE(1)=2
COINTICK=0
FOR I=0 TO 7
 FOR P=0 TO 1
  READ DEFCOOR(I,P)
 NEXT P
NEXT I
CHARA(0,0)=0
CHARA(1,0)=1
FOR I=0 TO 7
 READ STAGE$(I),STAGEBG(I)
NEXT I
SEL=0

GAMEPAD 2

TITLE:
WAIT VBL
STOP
CLS
BG 0
BG SOURCE ROM(6)
BG COPY 0,0,20,16 TO 0,0
ATTR 0
TEXT 2,13,"PRESS A TO START"
REPEAT
 WAIT VBL
UNTIL BUTTON TAP(0,0) OR BUTTON TAP(1,0)
BG 1
BG SOURCE ROM(5)
BG COPY 0,0,20,18 TO 0,0
BG 0
WAIT VBL
FOR I=0 TO 4
 BG SCROLL 0,0 TO 5,6 STEP -1,-1
 BG SCROLL 8,0 TO 19,6 STEP 1,-1
 BG SCROLL 0,8 TO 5,15 STEP -1,1
 BG SCROLL 8,8 TO 19,15 STEP 1,1
 BG FILL 5-I,0 TO 8+I,19 CHAR 0
 BG FILL 0,5-I TO 19,8+1 CHAR 0
 WAIT 3
NEXT I

BEGINING:
WAIT VBL
CLS
SPRITE OFF
BG 0
BG SOURCE ROM(5)
BG COPY 0,0,20,18 TO 0,0
SOUND SOURCE ROM(15)
MUSIC
ATTR(0,,,1,)
IMPULSE(0)=0
IMPULSE(1)=0
I=0
FOR Y=16 TO 64 STEP 48
 FOR X=16 TO 136 STEP 40
  SPRITE I,X,Y,6+I
  IF I<7 THEN SPRITE.A I,(I+1,,,,0) ELSE SPRITE.A I,(0,,,,0)
  I=I+1
 NEXT X
NEXT Y
 TEXT 0,12,"PLAYER1"
 TEXT 13,12,"PLAYER2"
 TEXT 6,14,"A-START"
 TEXT 5,15,"B-SETTINGS"
 DO
  FOR I=0 TO 1
   IF BUTTON TAP(I,0) THEN GOTO GAME
   IF BUTTON TAP(I,1) THEN GOTO SETTINGS
   IF LEFT TAP(I) THEN CHARA(I,0)=(CHARA(I,0)+7) MOD 8
   IF RIGHT TAP(I) THEN CHARA(I,0)=(CHARA(I,0)+1) MOD 8
   IF UP TAP(I) OR DOWN TAP(I) THEN CHARA(I,0)=(CHARA(I,0)+4) MOD 8
  NEXT I
  ATTR ((CHARA(0,0)+1) MOD 8,,,,)
  CELL 8,12,CHARA(0,0)+6
  ATTR ((CHARA(1,0)+1) MOD 8,,,,)
  CELL 11,12,CHARA(1,0)+6
  ATTR (0,,,,)
  FOR I=0 TO 7
   I$="   "
   IF I=CHARA(0,0) THEN I$=" 1 "
   IF I=CHARA(1,0) THEN I$=" 2 "
   IF I=CHARA(0,0) AND I=CHARA(1,0) THEN I$="1 2"
   TEXT 1+(I MOD 4)*5,3+(I\4)*6,I$
  NEXT I
  WAIT VBL
 LOOP

GAME:
FOR P=0 TO 8
 SPRITE OFF P
NEXT P
SOUND SOURCE ROM(4)
MUSIC
CHARA(0,1)=CHARA(0,0)
CHARA(1,1)=CHARA(1,0)
IF MODE(0)=0 THEN
 SCORE(0)=MODE(1)
 SCORE(1)=MODE(1)
ELSE
 TIME=TIMER
 IF MODE(0)=2 THEN COINTICK=TIMER
 SCORE(0)=0
 SCORE(1)=0
END IF
CLS
BG SOURCE ROM(STAGEBG(S))
BG COPY 0,0,20,18 TO 0,0
BG 1
FOR P=0 TO 1
 IF CHARA(P,0)<7 THEN ATTR (CHARA(P,0)+1,0,0,,) ELSE ATTR (0,0,0,,)
 IF MODE(0)=0 AND MODE(1)>2 THEN
  IF P=0 THEN
   BG FILL 0,13 TO MODE(1)-3,13 CHAR 6+CHARA(P,0)
  ELSE
   BG FILL 19-(MODE(1)-3),13 TO 19,13 CHAR 6+CHARA(P,0)
  END IF
  BG FILL 17*P,14 TO 2+(17*P),14 CHAR 6+CHARA(P,0)
  ATTR (0,,,,)
  BG FILL 17*P,12 TO 2+(17*P),12 CHAR 15
  IF P=0 THEN ATTR (,1,,,)
  BG FILL 3+(13*P),13 TO 3+(13*P),15 CHAR 14
 ELSE
  IF P=0 AND MODE(0)=0 THEN
   BG FILL 0,14 TO MODE(1),14 CHAR 6+CHARA(P,0)
  ELSE IF MODE(0)=0 THEN
   BG FILL 19-MODE(1),14 TO 19,14 CHAR 6+CHARA(P,0)
  ELSE
   CELL 19*P,14,6+CHARA(P,0)
  END IF
  ATTR (0,,,,)
  BG FILL 17*P,13 TO 2+(17*P),13 CHAR 15
  IF P=0 THEN ATTR (,1,,,)
  BG FILL 3+(13*P),14 TO 3+(13*P),15 CHAR 14
 END IF
 JUMP(P,0)=0
 FLOOR_COLLIDE(P)=0
NEXT P
BG 0
ATTR (0,,,,)
COORDINATES(0,1)=0
COORDINATES(1,1)=0
 COORDINATES(0,0)=DEFCOOR(S,0)
 COORDINATES(1,0)=DEFCOOR(S,1)
SPRITE 0,0,0,32+(8*CHARA(0,0))
IF CHARA(0,0)<7 THEN SPRITE.A 0,(CHARA(0,0)+1,0,0,,1) ELSE SPRITE.A 0,(0,0,0,,1)
FLIPED(0)=SPRITE.A(0)
SPRITE 1,104,0,32+(2*CHARA(1,0))
IF CHARA(1,0)<7 THEN SPRITE.A 1,(CHARA(1,0)+1,0,0,,1) ELSE SPRITE.A 1,(0,0,0,,1)
FLIPED(1)=SPRITE.A(1)
COINSPRITE=6
IF MODE(0)=2 THEN
 FOR I=1 TO 10
  REPEAT
   SPRITE COINSPRITE,INT(RND*152),INT(RND*120),224
  UNTIL NOT SPRITE HIT(COINSPRITE)
  SPRITE.A COINSPRITE,(0,,,,)
  COINSPRITE=COINSPRITE+1
 NEXT I
END IF
DO
 IF (MODE(0)=1 OR MODE(0)=2) AND TIMER-TIME>=MODE(1)*60 THEN
  IF SCORE(0)>SCORE(1) THEN WINNER=0 ELSE IF SCORE(0)=SCORE(1) THEN WINNER=3 ELSE WINNER=1
  GOTO WIN
 END IF
 IF MODE(0)=2 AND TIMER-COINTICK=300 THEN
  COINTICK=TIMER
  SPRITE COINSPRITE,INT(RND*152),INT(RND*120),224
  COINSPRITE=COINSPRITE+1
  IF COINSPRITE=64 THEN COINSPRITE=6
 END IF
 FOR P=0 TO 1
  IF PAUSE THEN GOSUB PAUSED
  TRACE COINTICK
  IF DEFECT(P,1,1)>0 THEN SPRITE.A P,(,,1,,) ELSE SPRITE.A P,(,,0,,)
  BG 1
  IF MODE(0)=1 OR MODE(0)=2 THEN
   NUMBER 0,0,(MODE(1)-(TIMER-TIME)\60)\60,2
   TEXT 2,0,":"
   NUMBER 3,0,(MODE(1)-(TIMER-TIME)\60) MOD 60,2
   NUMBER 1+(16*P),14,SCORE(P),2
  END IF
  NUMBER 17*P,15,IMPULSE(P),3
  BG 0
  IF COORDINATES(P,0)>-8 AND COORDINATES(P,1)>=0 AND COORDINATES(P,1)<112 THEN GOSUB CHECK_FLOOR ELSE FLOOR_COLLIDE(P)=0
  SPRITE P,COORDINATES(P,0),COORDINATES(P,1),
  IF COORDINATES(P,0)<-32 OR COORDINATES(P,0)>175 THEN GOSUB KO
  IF FLOOR_COLLIDE(P)=0 AND JUMP(P,0)=0 THEN COORDINATES(P,1)=COORDINATES(P,1)+1
  IF LEFT(P) AND BATTACK(P,1)=0 AND PUNCH(P)=0 AND DEFECT(P,1,1)=0 THEN 
   COORDINATES(P,0)=COORDINATES(P,0)-1
   SPRITE.A P,(,1,,,)
   WALKTICK(P)=WALKTICK(P)+1
   IF WALKTICK(P)>=15 AND JUMP(P,0)=0 THEN
    SPRITE P,,,(CHARA(P,0)*2+64)+(32*(TIMER MOD 2))
    WALKTICK(P)=0
   END IF
  ELSE IF RIGHT(P) AND BATTACK(P,1)=0 AND PUNCH(P)=0 AND DEFECT(P,1,1)=0 THEN
   COORDINATES(P,0)=COORDINATES(P,0)+1
   SPRITE.A P,(,0,,,)
   WALKTICK(P)=WALKTICK(P)+1
   IF WALKTICK(P)>=15 AND JUMP(P,0)=0 THEN
    SPRITE P,,,(CHARA(P,0)*2+64)+(32*(TIMER MOD 2))
    WALKTICK(P)=0
   END IF
  ELSE
   SPRITE P,,,32+(2*CHARA(P,0))
  END IF
  IF UP(P) AND JUMP(P,0)=0 AND BATTACK(P,1)=0 AND FLOOR_COLLIDE(P)=1 AND DEFECT(P,1,1)=0 THEN
    JUMP(P,0)=1
    JUMP(P,1)=COORDINATES(P,1)-32
  END IF
  IF DOWN(P) AND BATTACK(P,1)=0 AND JUMP(P,0)=0 AND DEFECT(P,1,1)=0 THEN COORDINATES(P,1)=COORDINATES(P,1)+1
  IF BUTTON TAP (P,0) AND PUNCH(P)=0 AND DEFECT(P,1,1)=0 THEN PUNCH(P)=10
  IF BUTTON TAP (P,1) AND BATTACK(P,0)=0 AND (BTICK(P)>=10 OR CHARA(P,0)=7) AND DEFECT(P,1,1)=0 THEN GOSUB BPUNCH
  IF PUNCH(P)>0 THEN
   IF SPRITE.A(P)=FLIPED(P) THEN SPRITE P+2,SPRITE.X(P)+16,SPRITE.Y(P),SPRITE.C(P)-16 ELSE SPRITE P+2,SPRITE.X(P)-8,SPRITE.Y(P),SPRITE.C(P)-16
   IF SPRITE.A(P)=FLIPED(P) THEN SPRITE.A P+2,(,0,,,) ELSE SPRITE.A P+2,(,1,,,)
   IF CHARA(P,0)=0 THEN
    SPRITE.A P+2,(3,,,,)
   ELSE IF CHARA(P,0)=3 OR CHARA(P,0)=6 OR CHARA(P,0)=7 THEN
    SPRITE.A P+2,(1,,,,)
   ELSE
    SPRITE.A P+2,(CHARA(P,0)+1,,,,)
   END IF
   IF CHARA(P,0)=6 THEN
    WALKTICK(P)=WALKTICK(P)+1
    IF WALKTICK(P)>=3 THEN 
     SPRITE.A P+2,(,,TIMER MOD 2,,)
     WALKTICK(P)=0
    END IF
   END IF
   PUNCH(P)=PUNCH(P)-1
   SPRITE P,,,160+(CHARA(P,0)*2)
   IF CHARA(P,0)<>6 THEN SPRITE.A P+2,(,,0,,)
  END IF
  IF PUNCH(P)=0 THEN SPRITE OFF P+2
  IF SPRITE HIT(P,2 TO 3) THEN
   IF DEFECT(P,2,1)=0 THEN IMPULSE(P)=IMPULSE(P)+1 ELSE IMPULSE(P)=IMPULSE(P)+0.4
   IF P=0 THEN IMPULSY=1 ELSE IMPULSY=0
   IF BATTACK(P,0)=0 AND DEFECT(P,1,1)=0 THEN
    BTICK(IMPULSY)=BTICK(IMPULSY)+1
   END IF
   IF DEFECT(P,2,1)=0 THEN
    IF SPRITE.A(IMPULSY)=FLIPED(IMPULSY) THEN COORDINATES(P,0)=COORDINATES(P,0)+7+IMPULSE(P)*(DEFECT(P,0,0)+1) ELSE COORDINATES(P,0)=COORDINATES(P,0)-IMPULSE(P)*(DEFECT(P,0,0)+1)
    COORDINATES(P,1)=COORDINATES(P,1)-7-INT(IMPULSE(P)/2)*(DEFECT(P,0,0)+1)
   END IF
   JUMP(P,0)=0
   DEFECT(P,0,0)=0
   DEFECT(P,0,1)=0
  END IF
  IF MODE(0)=2 AND SPRITE HIT(P,6 TO 63) THEN
   SCORE(P)=SCORE(P)+1
   SPRITE OFF HIT
  END IF
  IF JUMP(P,0)=1 AND COORDINATES(P,1)=JUMP(P,1) THEN JUMP(P,0)=0
  IF JUMP(P,0)=1 THEN
   COORDINATES(P,1)=COORDINATES(P,1)-1
   IF PUNCH(P)=0 AND BATTACK(P,0)=0 THEN SPRITE P,,,128+(CHARA(P,0)*2)
  END IF
  IF COORDINATES(P,1)>=128 THEN
   IMPULSE(P)=IMPULSE(P)+5
   COORDINATES(P,1)=0
  END IF
  IF BATTACK(P,0)=1 THEN
   IF CHARA(P,0)=5 THEN GOSUB KIRBY
   IF CHARA(P,0)=0 THEN GOSUB MARIO
   IF CHARA(P,0)=1 THEN GOSUB KONG
   IF CHARA(P,0)=2 THEN GOSUB LINK
   IF CHARA(P,0)=3 THEN GOSUB SAMUS
   IF CHARA(P,0)=4 THEN GOSUB YOSHI
   IF CHARA(P,0)=6 THEN GOSUB PIKACHU
   IF CHARA(P,0)=7 THEN GOSUB FOX
  END IF
  IF SPRITE.A(P)=FLIPED(P) AND BATTACK(P,0)=1 AND (CHARA(P,0)=0 OR CHARA(P,0)=2 OR (CHARA(P,0)>=4 AND CHARA(P,0)<=6)) THEN
   SPRITE P+4,COORDINATES(P,0)+16,COORDINATES(P,1),
  ELSE IF BATTACK(P,0)=1 AND (CHARA(P,0)=0 OR CHARA(P,0)=2 OR (CHARA(P,0)>=4 AND CHARA(P,0)<=6)) THEN
   SPRITE P+4,COORDINATES(P,0)-8,COORDINATES(P,1),
  END IF
  IF DEFECT(P,0,0)=1 THEN
   DEFECT(P,0,1)=DEFECT(P,0,1)-1
   IF DEFECT(P,0,1)=0 THEN DEFECT(P,0,0)=0
  END IF
  IF DEFECT(P,1,1)>0 THEN
   DEFECT(P,1,1)=DEFECT(P,1,1)-1
   IF BATTACK(P,0)=1 AND (CHARA(P,0)<>7 AND CHARA(P,0)<>1 AND CHARA(P,0)<>3) THEN
    BATTACK(P,0)=0
    SPRITE OFF P+4
   END IF
  END IF
  IF DEFECT(P,2,1)>0 THEN DEFECT(P,2,1)=DEFECT(P,2,1)-1
 NEXT P
 WAIT VBL
LOOP
KO:
BG 1
IF MODE(0)=0 THEN
 SCORE(P)=SCORE(P)-1
 IF P=0 THEN
  IF SCORE(P)+1>2 THEN CELL SCORE(P)-2,13,0 ELSE CELL SCORE(P)+1,14,0
 ELSE
  IF SCORE(P)+1>2 THEN CELL 19-(SCORE(P)-2),13,0 ELSE CELL 19-(SCORE(P)+1),14,0
 END IF
 IF SCORE(P)<0 THEN
  IF P=0 THEN WINNER=1 ELSE WINNER=0
  GOTO WIN
 END IF
ELSE IF MODE(0)=1 THEN
 IF P=0 THEN SCORE(1)=SCORE(1)+1 ELSE SCORE(0)=SCORE(0)+1
ELSE
 IF P=0 THEN I=1 ELSE I=0
 IF SCORE(P)<2 THEN
  SCORE(I)=SCORE(I)+SCORE(P)
  SCORE(P)=0
 ELSE
  SCORE(I)=SCORE(I)+2
  SCORE(P)=SCORE(P)-2
 END IF
END IF
COORDINATES(P,1)=0
COORDINATES(P,0)=DEFCOOR(S,P)
IMPULSE(P)=0
RETURN

WIN:
 STOP
WAIT 80
SPRITE 0,-16,-16,
SPRITE 1,-16,-16,
SPRITE OFF 2 TO 63
BG FILL 13,0 TO 20,8 CHAR 1
WAIT 10
BG FILL 0,9 TO 10,16 CHAR 1
WAIT 10
BG FILL 11,9 TO 20,16 CHAR 1
WAIT 10
BG FILL 0,0 TO 12,8 CHAR 1
WAIT 30
CLS
PRINT
BG SOURCE ROM(14)
BG COPY 0,0,20,16 TO 0,0
IF WINNER=1 THEN
 ATTR(1,,,0,)
 BG FILL 7,12 TO 10,15 CHAR 3
 BG FILL 0,11 TO 3,15 CHAR 2
 BG FILL 4,12 TO 6,15 CHAR 2
 ATTR(0,,,1,)
 PRINT "PLAYER 2 WINS!"
ELSE IF WINNER=0 THEN
 PRINT "PLAYER 1 WINS!"
ELSE
 ATTR(6,,,,)
 BG FILL 0,11 TO 3,11 CHAR 1
 BG FILL 0,12 TO 10,15 CHAR 1
 ATTR(0,,,,)
 PRINT "NO CONTEST"
END IF
IF WINNER<3 THEN 
 SPRITE WINNER,16,100,
 IF WINNER=1 THEN SPRITE 0,64,104, ELSE SPRITE 1,64,104,
END IF
REPEAT
 WAIT VBL
UNTIL BUTTON TAP(0,0) OR BUTTON TAP(1,0)
REM BIG 32,104 SMALL 72,120
GOTO BEGINING
CHECK_FLOOR:
  IF CELL.C(COORDINATES(P,0)\8,COORDINATES(P,1)\8+2)>=4 OR CELL.C(COORDINATES(P,0)\8+1,COORDINATES(P,1)\8+2)>=4 OR (COORDINATES(P,0) MOD 16>0 AND CELL.C(COORDINATES(P,0)\8+2,COORDINATES(P,1)\8+2)>=4) THEN FLOOR_COLLIDE(P)=1 ELSE FLOOR_COLLIDE(P)=0
 RETURN
 
SETTINGS:
ATTR(0,,,,)
SPRITE OFF
CLS
WAIT VBL
BG FILL 9,0 TO 10,15 CHAR 1
BG FILL 0,7 TO 19,8 CHAR 1
TEXT 2,15,"A-SELECT  B-BACK"
UPDSETTINGS:
WAIT VBL
IF SEL=0 THEN ATTR 5 ELSE ATTR 0
TEXT 2,2,"STAGE"
TEXT 0,3,"SELECTION"
IF SEL=1 THEN ATTR 5 ELSE ATTR 0
TEXT 13,2,"RULES"
IF SEL=2 THEN ATTR 5 ELSE ATTR 0
TEXT 3,11,"HOW"
TEXT 3,12,"TO"
TEXT 3,13,"PLAY"
IF SEL=3 THEN ATTR 5 ELSE ATTR 0
TEXT 13,11,"TITLE"
DO
 IF UP TAP(0) OR DOWN TAP(0) THEN
  SEL=(SEL+2) MOD 4
  GOTO UPDSETTINGS
 END IF
 IF LEFT TAP(0) THEN
  SEL=(SEL+3) MOD 4
  GOTO UPDSETTINGS
 END IF
 IF RIGHT TAP(0) THEN
  SEL=(SEL+1) MOD 4
  GOTO UPDSETTINGS
 END IF
 IF BUTTON TAP(0,0) THEN
  IF SEL=0 THEN GOTO STAGESELECC
  IF SEL=1 THEN GOTO RULES
  IF SEL=2 THEN GOTO INFO
  IF SEL=3 THEN GOTO TITLE
 END IF
 IF BUTTON TAP(0,1) THEN GOTO BEGINING
 WAIT VBL
LOOP

STAGESELECC:
WAIT VBL
CLS
BG 1
BG SOURCE ROM(STAGEBG(S))
BG COPY 0,0,20,18 TO 0,0
BG 0
ATTR (,,,1,)
FOR I=0 TO 7
 IF S=I THEN ATTR (5,,,,) ELSE ATTR (0,,,,)
 PRINT I+1;". ";STAGE$(I)
NEXT I
ATTR (0,,,,)
TEXT 6,15,"A-ACCEPT"
DO
 IF UP TAP(0) THEN
  S=(S+7) MOD 8
  GOTO STAGESELECC
 END IF
 IF DOWN TAP(0) THEN
  S=(S+1) MOD 8
  GOTO STAGESELECC
 END IF
 IF BUTTON TAP(0,0) THEN GOTO SETTINGS
 WAIT VBL
LOOP

RULES:
WAIT VBL
CLS
TEXT 8,0,"MODE"
TEXT 6,15,"A-ACCEPT"
TEXT 0,3,"<--"
TEXT 17,3,"-->"
BG FILL 0,7 TO 19,8 CHAR 1
DO
 TRACE MODE(1)
 IF MODE(0)=0 THEN
  TEXT 7,3,"STOCKS"
 ELSE IF MODE(0)=1 THEN
  TEXT 4,3,"TIME BATTLE"
 ELSE
  TEXT 4,3,"COIN BATTLE"
 END IF
 IF LEFT TAP(0) THEN
  TEXT 4,3,"           "
  MODE(0)=MODE(0)-1
  IF MODE(0)=-1 THEN MODE(0)=2
  IF MODE(0)=0 THEN MODE(1)=2 ELSE MODE(1)=300
 ELSE IF RIGHT TAP(0) THEN
  TEXT 4,3,"           "
  MODE(0)=MODE(0)+1
  IF MODE(0)=3 THEN MODE(0)=0
  IF MODE(0)=0 THEN MODE(1)=2 ELSE MODE(1)=300
 END IF
 IF MODE(0)>=1 THEN
  BG FILL 6,10 TO 12,12 CHAR 0
  TEXT 9,10,"+"
  NUMBER 7,11,MODE(1)\60,2
  TEXT 9,11,":"
  NUMBER 10,11,MODE(1) MOD 60,2
  TEXT 9,12,"-"
  IF UP TAP(0) THEN
   IF MODE(1)\60<60 THEN MODE(1)=MODE(1)+30
  ELSE IF DOWN TAP(0) THEN
   IF MODE(1)\60>0 THEN MODE(1)=MODE(1)-30
  END IF
 ELSE
  BG FILL 7,10 TO 12,12 CHAR 0
  TEXT 9,10,"+"
  ATTR(7,,,,)
  BG FILL 9-(MODE(1)\2),11 TO 9+(MODE(1)\2)+(MODE(1) MOD 2),11 CHAR 12
  ATTR(0,,,,)
  TEXT 9,12,"-"
  IF UP TAP(0) AND MODE(1)<5 THEN
   MODE(1)=MODE(1)+1
  ELSE IF DOWN TAP(0) AND MODE(1)>0 THEN
   MODE(1)=MODE(1)-1
  END IF
 END IF
 IF BUTTON TAP(0,0) THEN GOTO SETTINGS
 WAIT VBL
LOOP

PAUSED:
BG 0
TEXT 0,2,"A-RESUME"
TEXT 0,3,"B-QUIT"
REPEAT
 IF BUTTON TAP(0,1) OR BUTTON TAP(1,1) THEN
  WINNER=3
  GOTO WIN
 END IF
 WAIT VBL
UNTIL BUTTON TAP(0,0) OR BUTTON TAP(1,0)
BG FILL 0,2 TO 9,3 CHAR 0
BG 1
RETURN
BPUNCH:
 IF CHARA(P,0)=5 THEN
  BATTACK(P,0)=1
  BATTACK(P,1)=60
  IF SPRITE.A(P)=FLIPED(P) THEN
   SPRITE P+4,COORDINATES(P,0)+16,COORDINATES(P,1),27
   SPRITE.A P+4,(0,1,,,)
  ELSE
   SPRITE P+4,COORDINATES(P,0)-8,COORDINATES(P,1),27
   SPRITE.A P+4,(0,0,,,)
  END IF
 END IF
 IF CHARA(P,0)=0 THEN
  BATTACK(P,0)=1
  BATTACK(P,1)=60
  IF SPRITE.A(P)=FLIPED(P) THEN
   SPRITE P+4,COORDINATES(P,0)+16,COORDINATES(P,1),17
   SPRITE.A P+4,(1,0,,,)
  ELSE
   SPRITE P+4,COORDINATES(P,0)-8,COORDINATES(P,1),17
   SPRITE.A P+4,(1,1,,,)
  END IF
  RETURN
 END IF
 IF CHARA(P,0)=1 THEN
  BATTACK(P,0)=1
  SPRITE P,,,162
  BTICK(P)=0
  IF SPRITE.A(P)=FLIPED(P) THEN
   SPRITE P+4,COORDINATES(P,0)+16,COORDINATES(P,1),19
   MISILE(P,1)=2
  ELSE
   SPRITE P+4,COORDINATES(P,0)-8,COORDINATES(P,1),19
   MISILE(P,1)=-2
  END IF
  MISILE(P,0)=0
  SPRITE.A P+4,(7,,,,)
  RETURN
 END IF
 IF CHARA(P,0)=2 THEN
  BATTACK(P,0)=1
  BATTACK(P,1)=60
  IF SPRITE.A(P)=FLIPED(P) THEN
   SPRITE P+4,COORDINATES(P,0)+16,COORDINATES(P,1),21
   SPRITE.A P+4,(7,0,,,)
  ELSE
   SPRITE P+4,COORDINATES(P,0)-8,COORDINATES(P,1),21
   SPRITE.A P+4,(7,1,,,)
  END IF
  RETURN
 END IF
 IF CHARA(P,0)=3 THEN
  BATTACK(P,0)=1
  IF SPRITE.A(P)=FLIPED(P) THEN
   SPRITE P+4,COORDINATES(P,0)+16,COORDINATES(P,1),23
  ELSE
   SPRITE P+4,COORDINATES(P,0)-8,COORDINATES(P,1),23
  END IF
  SPRITE P,,,166
  MISILE(P,1)=SPRITE.Y(P+4)
  SPRITE.A P+4,(4,,,,)
  RETURN
 END IF
 IF CHARA(P,0)=4 THEN
  BATTACK(P,0)=1
  BATTACK(P,1)=60
  IF SPRITE.A(P)=FLIPED(P) THEN
   SPRITE P+4,COORDINATES(P,0)+16,COORDINATES(P,1),25
   SPRITE.A P+4,(5,0,,,)
  ELSE
   SPRITE P+4,COORDINATES(P,0)-8,COORDINATES(P,1),25
   SPRITE.A P+4,(5,1,,,)
  END IF
  RETURN
 END IF
 IF CHARA(P,0)=6 THEN
  BATTACK(P,0)=1
  BATTACK(P,1)=60
  IF SPRITE.A(P)=FLIPED(P) THEN
   SPRITE P+4,COORDINATES(P,0)+16,COORDINATES(P,1),29
   SPRITE.A P+4,(1,0,,,)
  ELSE
   SPRITE P+4,COORDINATES(P,0)-8,COORDINATES(P,1),29
   SPRITE.A P+4,(1,1,,,)
  END IF
  RETURN
 END IF
 IF CHARA(P,0)=7 THEN
  BATTACK(P,0)=1
  IF SPRITE.A(P)=FLIPED(P) THEN
   MISILE(P,0)=COORDINATES(P,0)+16
   MISILE(P,1)=1
  ELSE
   MISILE(P,0)=COORDINATES(P,0)-8
   MISILE(P,1)=0
  END IF
  SPRITE P,,,174
  SPRITE P+4,MISILE(P,0),COORDINATES(P,1),31
  SPRITE.A P+4,(1,,,,)
  RETURN
 END IF
RETURN

MARIO:
 BATTACK(P,1)=BATTACK(P,1)-1
 SPRITE P,,,160
 IF BATTACK(P,1)=0 THEN
  SPRITE OFF P+4
  BATTACK(P,0)=0
 END IF
 IF SPRITE HIT(P+4, 0 TO 1) THEN
  IMPULSE(HIT)=IMPULSE(HIT)+24
  BATTACK(P,0)=0
  BATTACK(P,1)=0
  BTICK(P)=0
  SPRITE P+2,SPRITE.X(P+4),SPRITE.Y(P+4),17
  SPRITE.A P+2,(1,,,,)
  SPRITE OFF P+4
 END IF
 IF BATTACK(P,0)=0 AND CHARA(P,1)=5 THEN 
  WAIT 5
  CHARA(P,0)=5
 END IF
RETURN

PIKACHU:
 BATTACK(P,1)=BATTACK(P,1)-1
 SPRITE P,,,172
 IF BATTACK(P,1)=0 THEN
  SPRITE OFF P+4
  BATTACK(P,0)=0
 END IF
 IF SPRITE HIT(P+4,0 TO 1) THEN
  IMPULSE(HIT)=IMPULSE(HIT)+2
  BATTACK(P,0)=0
  BATTACK(P,1)=0
  BTICK(P)=0
  DEFECT(HIT,0,0)=1
  DEFECT(HIT,0,1)=1
  SPRITE P+2,SPRITE.X(P+4),SPRITE.Y(P+4),29
  SPRITE OFF P+4
 END IF
 IF BATTACK(P,0)=0 AND CHARA(P,1)=5 THEN 
  WAIT 5
  CHARA(P,0)=5
 END IF
RETURN

KIRBY:
 BATTACK(P,1)=BATTACK(P,1)-1
 SPRITE P,,,170
 IF BATTACK(P,1)=0 THEN
  SPRITE OFF P+4
  BATTACK(P,0)=0
 END IF
 IF SPRITE HIT(P+4, 0 TO 1) THEN
  BATTACK(P,0)=1
  BATTACK(P,1)=0
  CHARA(P,0)=CHARA(HIT,1)
  IF CHARA(HIT,1)=5 THEN CHARA(P,0)=INT(RND*7)
  GOSUB BPUNCH
 END IF
RETURN

LINK:
 BATTACK(P,1)=BATTACK(P,1)-1
 SPRITE P,,,164
 IF BATTACK(P,1)=0 THEN
  SPRITE OFF P+4
  BATTACK(P,0)=0
 END IF
 IF SPRITE HIT(P+4, 0 TO 1) THEN
  IMPULSE(HIT)=IMPULSE(HIT)+2
  BATTACK(P,0)=0
  BATTACK(P,1)=0
  BTICK(P)=0
  DEFECT(HIT,0,0)=1
  DEFECT(HIT,0,1)=300
  DEFECT(HIT,1,1)=300
  COORDINATES(HIT,1)=COORDINATES(HIT,1)-8
  SPRITE OFF P+4
 END IF
 IF BATTACK(P,0)=0 AND CHARA(P,1)=5 THEN 
  WAIT 5
  CHARA(P,0)=5
 END IF
RETURN

YOSHI:
BATTACK(P,1)=BATTACK(P,1)-1
 SPRITE P,,,40
 IF BATTACK(P,1)=0 THEN
  SPRITE OFF P+4
  BATTACK(P,0)=0
 END IF
 IF SPRITE HIT(P+4, 0 TO 1) THEN
  IF SPRITE.A(P)=FLIPED(P) THEN COORDINATES(HIT,0)=COORDINATES(P,0)-16 ELSE COORDINATES(HIT,0)=COORDINATES(P,0)+16
  BATTACK(P,0)=0
  BATTACK(P,1)=0
  BTICK(P)=0
  DEFECT(HIT,1,1)=INT(IMPULSE(HIT)*6)
  DEFECT(HIT,2,1)=INT(IMPULSE(HIT)*6)
  SPRITE OFF P+4
 END IF
 IF BATTACK(P,0)=0 AND CHARA(P,1)=5 THEN 
  WAIT 5
  CHARA(P,0)=5
 END IF
RETURN

FOX:
 IF MISILE(P,1)=1 THEN MISILE(P,0)=MISILE(P,0)+2 ELSE MISILE(P,0)=MISILE(P,0)-2
 SPRITE P+4,MISILE(P,0),,
 IF MISILE(P,0)<=-8 OR MISILE(P,0)>=160 THEN
  BATTACK(P,0)=0
  SPRITE OFF P+4
 END IF
 IF SPRITE HIT(P+4,0 TO 1) THEN
  BATTACK(P,0)=0
  SPRITE P+2,SPRITE.X(P+4),SPRITE.Y(P+4),30
  SPRITE.A P+2,(4,,,,)
  SPRITE OFF P+4
 END IF
 IF BATTACK(P,0)=0 AND CHARA(P,1)=5 THEN 
  WAIT 5
  CHARA(P,0)=5
 END IF
RETURN

SAMUS:
 IF CELL.C(SPRITE.X(P+4)\8,MISILE(P,1)\8+1)>=4 OR (SPRITE.X(P+4)\8 MOD 8>0 AND CELL.C(SPRITE.X(P+4)\8+1,MISILE(P,1)\8+1)>=4) THEN MISILE(P,0)=1 ELSE MISILE(P,0)=0
 TRACE MISILE(P,0)
 IF MISILE(P,0)=0 THEN MISILE(P,1)=MISILE(P,1)+1
 IF MISILE(P,1)>=128 THEN
  BATTACK(P,0)=0
  SPRITE OFF P+4
 END IF
 SPRITE P+4,,MISILE(P,1),
 IF SPRITE HIT(P+4,0 TO 1) AND HIT<>P THEN
  DEFECT(HIT,0,0)=1
  DEFECT(HIT,0,1)=1
  BTICK(P)=0
  IMPULSE(HIT)=IMPULSE(HIT)+4
  SPRITE P+2,SPRITE.X(P+4),SPRITE.Y(P+4),23
  SPRITE.A P+2,(4,,,,)
  SPRITE OFF P+4
  BATTACK(P,0)=0
 END IF
 IF BATTACK(P,0)=0 AND CHARA(P,1)=5 THEN 
  WAIT 5
  CHARA(P,0)=5
 END IF
RETURN
KONG:
 SPRITE P+4,SPRITE.X(P+4)+MISILE(P,1),,
 BTICK(P)=BTICK(P)+1
 IF BTICK(P)=5 THEN
  SPRITE.A P+4,(,0,0,,)
 ELSE IF BTICK(P)=10 THEN
  SPRITE.A P+4,(,1,0,,)
 ELSE IF BTICK(P)=15 THEN
  SPRITE.A P+4,(,1,1,,)
 ELSE IF BTICK(P)=20 THEN
  SPRITE.A P+4,(,0,1,,)
  BTICK(P)=0
 END IF
 IF SPRITE.X(P+4)>=152 OR SPRITE.X(P+4)<=0 THEN
  IF MISILE(P,0)=1 THEN
   BATTACK(P,0)=0
   SPRITE OFF P+4
   BTICK(P)=10
  ELSE
   MISILE(P,0)=1
   MISILE(P,1)=MISILE(P,1)*-1
  END IF
 END IF
 IF SPRITE HIT (P+4,0 TO 1) THEN
  IF HIT=P OR CHARA(P,1)=5 THEN
   IMPULSE(P)=IMPULSE(P)-15
   IF IMPULSE(P)<0 THEN IMPULSE(P)=0
  ELSE
   IMPULSE(HIT)=IMPULSE(HIT)+15
   SPRITE P+2,SPRITE.X(P+4),SPRITE.Y(P+4),19
   SPRITE.A P+2,(7,,,,)
  END IF
  SPRITE OFF P+4
  BATTACK(P,0)=0
  BTICK(P)=0
 END IF
 IF BATTACK(P,0)=0 AND CHARA(P,1)=5 THEN 
  WAIT 5
  CHARA(P,0)=5
 END IF
RETURN

INFO:
WAIT VBL
ATTR(0,,,,)
CLS
PRINT "GUIDE:"
PRINT "MECHANICS:"
PRINT 
PRINT "BUILD UP DAMAGE TO  SEND YOUR OPPONENT  OFF THE STAGE!"
PRINT "THE MORE DAMAGE YOU HAVE THE MORE YOU'LLGET KNOCKED OFF!"
PRINT
