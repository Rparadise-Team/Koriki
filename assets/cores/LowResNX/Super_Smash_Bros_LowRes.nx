I=0
SPRITE.A 62,(3,1,,,0)
REPEAT
 IF I<=30 THEN
  SPRITE 63,,,160
  SPRITE 62,32,104,16
 ELSE
  SPRITE 63,,,32
  SPRITE 62,,,0
  IF I=60 THEN I=0
 END IF
 I=I+1
 WAIT VBL
UNTIL BUTTON TAP(0,0)
CLS
PRINT "B ATTACKS:"
PRINT
PRINT "THESE ATTACKS CAN BEUSED AFTER DOING TENSUCCESSFUL ATTACKS."
