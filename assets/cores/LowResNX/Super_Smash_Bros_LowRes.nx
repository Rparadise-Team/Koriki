IMPULSE(0)=0
SPRITE 63,16,104,32
SPRITE.A 63,(1,,,,1)
SPRITE 62,32,104,32
SPRITE.A 62,(1,1,,,1)
REPEAT
 IF NOT SPRITE HIT(63) THEN
  SPRITE 63,,,32
  SPRITE 62,SPRITE.X(62)-1,,
 ELSE
  SPRITE 63,,,160
  IMPULSE(0)=IMPULSE(0)+4
  SPRITE 62,SPRITE.X(62)+IMPULSE(0),SPRITE.Y(62)-INT(IMPULSE(0)/2),
 END IF
 IF SPRITE.Y(62)<104 THEN SPRITE 62,,SPRITE.Y(62)+1,
 WAIT VBL
UNTIL BUTTON TAP(0,0)
CLS
PRINT "ATTACKS:"
PRINT
PRINT "ATTACKS HAPPEN WHEN THE A BUTTON IS     PRESSED."
PRINT "EACH ATTACK DEALS   ONE DAMAGE."
PRINT
PRINT "A-CONTINUE"
