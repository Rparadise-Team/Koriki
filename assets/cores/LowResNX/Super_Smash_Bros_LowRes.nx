SPRITE OFF 62
WALKTICK(0)=0
I=0
REPEAT
 I=I+1
 IF I<=60 THEN
  SPRITE 63,SPRITE.X(63)+1,,
  SPRITE.A 63,(,0,,,)
 ELSE IF I<=120 THEN
  SPRITE 63,SPRITE.X(63)-1,,
  SPRITE.A 63,(,1,,,)
