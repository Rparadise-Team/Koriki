  SPRITE.A 63,(,0,,,)
  SPRITE 63,,SPRITE.Y(63)-1,142
 ELSE IF I<=184 THEN
  SPRITE 63,,SPRITE.Y(63)+1,46
  IF I=184 THEN I=0
 END IF
 WAIT VBL
UNTIL BUTTON TAP(0,0)
CLS
PRINT "STOCK BATTLES"
PRINT 
PRINT "BATTLE UNTIL YOU RUNOUT OF LIVES!"
PRINT 
PRINT "A-CONTINUE"
BG FILL 0,15 TO 19,15 CHAR 5
BG FILL 2,12 TO 3,12 CHAR 13
SPRITE 63,16,104,46
SPRITE.A 63,(,0,,,)
SPRITE 62,48,104,32
SPRITE.A 62,(,,,,1)
I=60
REPEAT
 IF SPRITE HIT(63) AND I=60 THEN
  SPRITE 63,-16,,
  SPRITE 62,,,160
  CELL 3,12,0
  I=0
 ELSE IF I=60 THEN
  SPRITE 62,SPRITE.X(62)-1,,
 ELSE
  I=I+1
 END IF
 IF I=60 AND SPRITE.X(63)=-16 THEN
  SPRITE 63,16,,
  SPRITE 62,48,,32
  CELL 3,12,13
 END IF
 WAIT VBL
UNTIL BUTTON TAP(0,0)
CLS
PRINT "TIME BATTLE:"
PRINT
PRINT "KNOCK OUT YOUR OPPONENT OUT OF THE STAGEAS MANY TIMES AS YOUCAN BEFORE TIME RUNSOUT!"
PRINT
PRINT "A-CONTINUE"
BG FILL 0,15 TO 19,15 CHAR 5
SPRITE 63,16,104,46
SPRITE.A 63,(,0,,,)
SPRITE 62,48,104,32
SPRITE.A 62,(,1,,,1)
I=60
REPEAT
 IF SPRITE HIT(63) AND I=60 THEN
  SPRITE 63,-16,,
  SPRITE 62,,,160
  TEXT 3,12,"+1"
  I=0
 ELSE IF I=60 THEN
  SPRITE 62,SPRITE.X(62)-1,,
 ELSE
  I=I+1
 END IF
 IF I=60 AND SPRITE.X(63)=-16 THEN
  SPRITE 63,16,,
  SPRITE 62,48,,32
  BG FILL 3,12 TO 4,12 CHAR 0
 END IF
 WAIT VBL
UNTIL BUTTON TAP(0,0)
CLS
PRINT "COIN BATTLES:"
PRINT
PRINT "COLLECT MORE COINS  THAN YOUR OPPONENT  TO WIN!"
PRINT
PRINT "A-EXIT"
BG FILL 0,15 TO 19,15 CHAR 5
SPRITE 63,16,104,46
SPRITE 62,48,112,224
SPRITE.A 62,(0,0,0,,0)
I=0
REPEAT
 IF SPRITE HIT(62) THEN
  SPRITE 62,-8,,
  I=0
 ELSE IF I=60 THEN
  SPRITE 63,SPRITE.X(63)+1,,
 ELSE
  I=I+1
 END IF
 IF I=60 AND SPRITE.X(62)=-8 THEN
  SPRITE 63,16,,
  SPRITE 62,48,,
 END IF
 WAIT VBL
UNTIL BUTTON TAP(0,0)
CLS
