SYNC1=0
TSYNC1=1
SYNC2=0
TSYNC2=1
SYNC3=0
TSYNC3=1


NUMBER 4,6,T0,2
NUMBER 14,6,T1,2
NUMBER 4,8,T2,2
NUMBER 14,8,T3,2

DO
  
  X=TOUCH.X\8
  Y=TOUCH.Y\8
  
  REM V0***
  
  IF TAP AND X>0 AND X<8 AND Y>0 AND Y<6 THEN
     TRACK T0,0
     PALETTE 0,,59,,
     TI0=TIMER
  END IF
  IF TIMER>TI0+1 THEN
     PALETTE 0,,55,,
  END IF
  IF TAP AND X>7 AND X<10 AND Y>0 AND Y<2 THEN
     SWAP M0,M01
     SWAP M01,M02
     VOLUME 0,,M0
     IF M0=3 THEN
        CELL 8,1,30
        CELL 9,1,47
     END IF
     IF M0=1 THEN
        CELL 8,1,30
        CELL 9,1,31
     END IF
     IF M0=2 THEN
        CELL 8,1,46
        CELL 9,1,47
     END IF
  END IF   
  IF TAP AND X>1 AND X<4 AND Y>5 AND Y<8 THEN
     T0=T0-1
     IF T0<0 THEN T0=63
     NUMBER 4,6,T0,2
  END IF
  IF TAP AND X>5 AND X<8 AND Y>5 AND Y<8 THEN
     T0=T0+1
     IF T0>63 THEN T0=0
     NUMBER 4,6,T0,2
  END IF
  IF TAP AND X>-1 AND X<2 AND Y>5 AND Y<7 THEN
     SWAP AUTO0,TAUTO0
     IF AUTO0=1 THEN
        ATTR 1
        CELL 0,6,18
        CELL 1,6,19
        ATTR 4
     ELSE
        ATTR 4
        CELL 0,6,18
        CELL 1,6,19
     END IF
  END IF
  IF TAP AND X>7 AND X<10 AND Y>2 AND Y<4 THEN
     SWAP SYNC0,TSYNC0
     IF SYNC0=1 THEN
        ATTR 1
        CELL 8,3,62
        CELL 9,3,63
        ATTR 4
     ELSE 
        ATTR 4
        CELL 8,3,62
        CELL 9,3,63
     END IF
  END IF
REM ***

REM V1***

  IF TAP AND X>11 AND X<19 AND Y>0 AND Y<6 THEN
     TRACK T1,1
     PALETTE 1,,31,,
     TI1=TIMER
  END IF
  IF TIMER>TI1+1 THEN
     PALETTE 1,,28,,
  END IF
  IF TAP AND X>9 AND X<12 AND Y>0 AND Y<2 THEN
     SWAP M1,M11
     SWAP M11,M12
     VOLUME 1,,M1
     IF M1=3 THEN
        CELL 10,1,30
        CELL 11,1,47
     END IF
     IF M1=1 THEN
        CELL 10,1,30
        CELL 11,1,31
     END IF
     IF M1=2 THEN
        CELL 10,1,46
        CELL 11,1,47
     END IF
  END IF   
  IF TAP AND X>15 AND X<18 AND Y>5 AND Y<8 THEN
     T1=T1+1
     IF T1>63 THEN T1=0
     NUMBER 14,6,T1,2
  END IF
  IF TAP AND X>11 AND X<15 AND Y>5 AND Y<8 THEN
     T1=T1-1
     IF T1<0 THEN T1=63
     NUMBER 14,6,T1,2
  END IF
  IF TAP AND X>17 AND X=19 AND Y>5 AND Y<7 THEN
     SWAP AUTO1,TAUTO1
     IF AUTO1=1 THEN
        ATTR 1
        CELL 18,6,18
        CELL 19,6,19
        ATTR 4
     ELSE
        ATTR 4
        CELL 18,6,18
        CELL 19,6,19
     END IF
  END IF
  
   IF TAP AND X>9 AND X<12 AND Y>2 AND Y<4 THEN
     SWAP SYNC1,TSYNC1
     IF SYNC1=1 THEN
        ATTR 1
        CELL 10,3,62
        CELL 11,3,63
        ATTR 4
     ELSE 
        ATTR 4
        CELL 10,3,62
        CELL 11,3,63
     END IF
  END IF
REM ***

REM V2***
  
  IF TAP AND X>0 AND X<8 AND Y>10 AND Y<15 THEN
     TRACK T2,2
     PALETTE 2,,52,,
     TI2=TIMER
  END IF
  IF TIMER>TI2+1 THEN
     PALETTE 2,,48,,
  END IF
   IF TAP AND X>7 AND X<10 AND Y>13 AND Y<15 THEN
     SWAP M2,M21
     SWAP M21,M22
     VOLUME 2,,M2
     IF M2=3 THEN
        CELL 8,14,30
        CELL 9,14,47
     END IF
     IF M2=1 THEN
        CELL 8,14,30
        CELL 9,14,31
     END IF
     IF M2=2 THEN
        CELL 8,14,46
        CELL 9,14,47
     END IF
  END IF   
  IF TAP AND X>1 AND X<4 AND Y>7 AND Y<10 THEN
     T2=T2-1
     IF T2<0 THEN T2=63
     NUMBER 4,8,T2,2
  END IF
  IF TAP AND X>5 AND X<8 AND Y>7 AND Y<10 THEN
     T2=T2+1
     IF T2>63 THEN T2=0
     NUMBER 4,8,T2,2
  END IF
  IF TAP AND X>-1 AND X<3 AND Y>8 AND Y<10 THEN
     SWAP AUTO2,TAUTO2
     IF AUTO2=1 THEN
        ATTR 1
        CELL 0,9,18
        CELL 1,9,19
        ATTR 4
     ELSE
        ATTR 4
        CELL 0,9,18
        CELL 1,9,19
     END IF
  END IF
  IF TAP AND X>7 AND X<10 AND Y>11 AND Y<13 THEN
     SWAP SYNC2,TSYNC2
     IF SYNC2=1 THEN
        ATTR 1
        CELL 8,12,62
        CELL 9,12,63
        ATTR 4
     ELSE 
        ATTR 4
        CELL 8,12,62
        CELL 9,12,63
     END IF
  END IF
REM ***

REM V3***

  IF TAP AND X>11 AND X<19 AND Y>10 AND Y<15 THEN
     TRACK T3,3
     TI3=TIMER
     PALETTE 3,,11,,
  END IF
  IF TIMER>TI3+1 THEN
     PALETTE 3,,7,,
  END IF
  IF TAP AND X>9 AND X<12 AND Y>13 AND Y<15 THEN
     SWAP M3,M31
     SWAP M31,M32
     VOLUME 3,,M3
     IF M3=3 THEN
        CELL 10,14,30
        CELL 11,14,47
     END IF
     IF M3=1 THEN
        CELL 10,14,30
        CELL 11,14,31
     END IF
     IF M3=2 THEN
        CELL 10,14,46
        CELL 11,14,47
     END IF
  END IF   
  IF TAP AND X>11 AND X<15 AND Y>7 AND Y<10 THEN
     T3=T3-1
     IF T3<0 THEN T3=63
     NUMBER 14,8,T3,2
  END IF
  IF TAP AND X>15 AND X<18 AND Y>7 AND Y<10 THEN
     T3=T3+1
     IF T3>63 THEN T3=0
     NUMBER 14,8,T3,2
  END IF
  IF TAP AND X>16 AND X=19 AND Y>8 AND Y<10 THEN
     SWAP AUTO3,TAUTO3
     IF AUTO3=1 THEN
        ATTR 1
        CELL 18,9,18
        CELL 19,9,19
        ATTR 4
     ELSE
        ATTR 4
        CELL 18,9,18
        CELL 19,9,19
     END IF
  END IF
  IF TAP AND X>9 AND X<12 AND Y>11 AND Y<13 THEN
     SWAP SYNC3,TSYNC3
     IF SYNC3=1 THEN
        ATTR 1
        CELL 10,12,62
        CELL 11,12,63
        ATTR 4
     ELSE 
        ATTR 4
        CELL 10,12,62
        CELL 11,12,63
     END IF
  END IF
        
REM ***
  
  IF AUTO0=1 AND SYNC0=0 THEN
     IF TIMER>TI0+256 THEN 
        TRACK T0,0
        PALETTE 0,,59,,
        TI0=TIMER
     END IF
     IF TIMER>TI0+3 THEN
        PALETTE 0,,55,,
     END IF
  END IF
  
  IF AUTO1=1 AND SYNC1=0 THEN
     IF TIMER>TI1+256 THEN 
        TRACK T1,1
        PALETTE 1,,31,,
        TI1=TIMER
     END IF
     IF TIMER>TI1+3 THEN
        PALETTE 1,,28,,
     END IF
  END IF
  
  IF AUTO2=1 AND SYNC2=0 THEN
     IF TIMER>TI2+256 THEN 
        TRACK T2,2
        PALETTE 2,,52,,
        TI2=TIMER
     END IF
     IF TIMER>TI2+3 THEN
        PALETTE 2,,48,,
     END IF
  END IF
  
  IF AUTO3=1 AND SYNC3=0 THEN
     IF TIMER>TI3+256 THEN 
        TRACK T3,3
        PALETTE 3,,11,,
        TI3=TIMER
     END IF
     IF TIMER>TI3+3 THEN
        PALETTE 3,,7,,
     END IF     
  END IF
  
  IF SYNC0=1 OR SYNC1=1 OR SYNC2=1 OR SYNC3=1 THEN
     IF TIMER>GLTIMER+256 THEN 
        ATTR 2
        CELL 9,7,42
        CELL 10,7,43
        CELL 9,8,58
        CELL 10,8,59
        ATTR 4
        GLTIMER=TIMER
        IF SYNC0=1 THEN TRACK T0,0
        IF SYNC1=1 THEN TRACK T1,1
        IF SYNC2=1 THEN TRACK T2,2
        IF SYNC3=1 THEN TRACK T3,3
     END IF
     IF TIMER>GLTIMER+3 THEN 
        CELL 9,7,0
        CELL 10,7,0
        CELL 9,8,0
        CELL 10,8,0
     END IF
  END IF
  
  
  WAIT VBL
  
LOOP


