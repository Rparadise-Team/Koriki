BG COPY 0,0,20,16 TO 0,0

TOUCHSCREEN

ATTR 4
FONT 64

T0=0
T1=1
T2=2
T3=3

AUTO0=0
AUTO1=0
AUTO2=0
AUTO3=0

TAUTO0=1
TAUTO1=1
TAUTO2=1
TAUTO3=1

TI0=TIMER
TI1=TIMER
TI2=TIMER
TI3=TIMER

M0=3
M01=2
M02=1
M1=3
M11=2
