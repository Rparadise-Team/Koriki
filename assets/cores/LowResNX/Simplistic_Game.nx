GAMEPAD 1


RANDOMIZE TIMER

X=1
Y=0
ILEVEL=0

CLS
PRINT
ATTR(3)
PRINT
PRINT "    SIMPLISTIC"
PRINT
PRINT 
PRINT "    BY HAMTARO"
PRINT
PRINT
PRINT "  STARING HAMTARO"
PRINT
PRINT
PRINT " (TAP A/B TO PLAY)"

WHILE NOT BUTTON(0)
WAIT VBL
WEND
DISPLAY(0,1,1,1,1)

DRAWLEVEL:
GOSUB UPDATEPLAYER
WAIT VBL
TRACK 0,1
WAIT 60
YBG=ILEVEL*8
BG 1
BG COPY 0,YBG,10,8 TO 0,0


DO

REM CHECK CELLS
IF CELL.C(X,Y)=8 THEN 
   ILEVEL=ILEVEL+1
   IF ILEVEL=7 THEN GOTO THE_END ELSE GOTO DRAWLEVEL
END IF

IWATER=0
IF CELL.C(X,Y)=12 AND CELL.C(X,Y-1)<>6 AND Y>0 THEN 
IWATER=1
Y=Y-1
END IF

WHILE CELL.C(X,Y)=14 
GOSUB UPDATEPLAYER
Y=Y-2
WAIT 5
GOSUB UPDATEPLAYER
WEND

WHILE CELL.C(X,Y)=32
GOSUB UPDATEPLAYER
Y=Y+3
WAIT 5
GOSUB UPDATEPLAYER
WEND


GOSUB UPDATEPLAYER


REM CHECK MOVEMENT
IF RIGHT(0) AND X<8 THEN 
IF CELL.C(X+1,Y)<>6 THEN
X=X+1
PLAY 0,85,1 SOUND 0
END IF
END IF

IF LEFT(0) AND X>1 THEN 
IF CELL.C(X-1,Y)<>6 AND CELL.C(X-1,Y)<>10 AND CELL.C(X,Y)<>10 THEN
X=X-1
PLAY 0,85,1 SOUND 0
END IF
END IF

IF UP(0) AND Y>0 THEN 
IF CELL.C(X,Y-1)<>6 AND CELL.C(X,Y)<>12 THEN
Y=Y-1
PLAY 0,85,1 SOUND 0
END IF
END IF

IF DOWN(0) AND Y<7 THEN 
IF CELL.C(X,Y+1)<>6 AND CELL.C(X,Y)<>12 AND CELL.C(X,Y+1)<>12 THEN
Y=Y+1
PLAY 0,85,1 SOUND 0
END IF
END IF

IF IWATER=0 THEN WAIT 10 ELSE WAIT 5
LOOP



REM 1=PLAYER (WHITE) CELL 4
REM 2=NOLEFT (GRAY) CELL 10
REM 6=WALL (BROWN) CELL 6
REM 7=YJUMP+3 (ORANGE) CELL 32
REM 8=EXIT (YELLOW) CELL 8
REM 11=FALLUP (LTBLUE) CELL 12
REM 14=YJUMP-2 (PINK) CELL 14



UPDATEPLAYER:
CLS 0
BG 0
ATTR(2)
CELL X,Y,4
BG 1
RETURN



THE_END:
CLS
DISPLAY(1,1,1,0,0)
ATTR(3)
PRINT
PRINT
PRINT "THANKS FOR PLAYING!"
END





