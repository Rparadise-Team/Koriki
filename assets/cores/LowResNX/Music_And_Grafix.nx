

BG COPY 0,0,19,15 TO 0,0
RANDOMIZE TIMER
DIM WX(63)
DIM WY(63)
DIM WYC(63)
DIM WYOS(63)
T=TIMER
S1=0 
S2=8
S3=14
FOR I=0 TO 63
    WX(I)=0
    WY(I)=0
    WYC(I)=((RND*5)/100)+0.1
    WYOS(I)=(RND*50)
    SPRITE.A I,(4)
NEXT I

TEXT 1,0," OF 3 SONGS"
DO 
  FOR I=0 TO 20
  WY(I)=WY(I)-WYC(I)
  IF I MOD 2=0 THEN
     WX(I)=WX(I)+1
  ELSE
     WX(I)=WX(I)-1
  END IF
  SPRITE I,72+WX(I),66+WYOS(I)*SIN(WY(I)),1
  IF 64+ABS(WY(I))>68 THEN 
     WY(I)=0
     WX(I)=0
     WYC(I)=((RND*5)/100)+0.1
     WYOS(I)=(RND*50)
  END IF
  NEXT I
  WAIT VBL
  IF TIMER=T+1 THEN 
     CALL NEW(S1)
     TEXT 0,0,"1"
  END IF
  IF TIMER=T+2400 THEN 
     CALL NEW(S2)
     TEXT 0,0,"2"
  END IF
  IF TIMER=T+4800 THEN 
     CALL NEW(S3)
     TEXT 0,0,"3"
  END IF
  IF TIMER=T+7200 THEN T=TIMER
    
LOOP

SUB NEW(S)
    MUSIC S
END SUB


