' WRITTEN BY PRANXTER IN 2019

GAMEPAD 1

DISPLAY (,,,1,)
BG COPY 0,120,32,16 TO 0,0
WHILE NOT BUTTON(0)
WAIT VBL
WEND

PX=16
PY=16
SPRITE.A 32,(1,,,,1)
LVL=0
BG COPY 0,LVL,32,16 TO 0,0

RESTART:
DO
  SKIP:
  SPRITE 32,PX,PY,32
  
  IF UP(0) THEN
    ISOUND=0
    WHILE PY>0 AND NOT CELL.C(PX/16,(PY-1)/16)=2 
      PY=PY-2
      ISOUND=1
      SPRITE 32,PX,PY,32
      IF CELL.C(PX/16,(PY+15)/16)=6 THEN GOTO WIN
      WAIT VBL
    WEND
    IF CELL.C(PX/16,(PY+15)/16)=8 THEN GOSUB PORTAL
    IF CELL.C(PX/16,(PY+15)/16)=10 THEN GOSUB SLIDEUP
    IF CELL.C(PX/16,(PY+15)/16)=12 THEN GOSUB SLIDEDOWN
    IF ISOUND=1 THEN PLAY 0,50,10 SOUND 5
    IF ISOUND=2 THEN PLAY 0,50,20 SOUND 1
    GOTO SKIP
  END IF
  
  IF DOWN(0) THEN 
    ISOUND=0
    WHILE PY<112 AND NOT CELL.C(PX/16,(PY+16)/16)=2
      PY=PY+2
      ISOUND=1
      SPRITE 32,PX,PY,32
      IF CELL.C(PX/16,PY/16)=6 THEN GOTO WIN
      WAIT VBL
    WEND
    IF CELL.C(PX/16,PY/16)=8 THEN GOSUB PORTAL
    IF CELL.C(PX/16,PY/16)=10 THEN GOSUB SLIDEUP
    IF CELL.C(PX/16,PY/16)=12 THEN GOSUB SLIDEDOWN
    IF ISOUND=1 THEN PLAY 0,50,10 SOUND 5
    IF ISOUND=2 THEN PLAY 0,50,20 SOUND 1
    GOTO SKIP
  END IF
  
  IF LEFT(0) THEN 
    ISOUND=0
    WHILE PX>0 AND NOT CELL.C((PX-1)/16,PY/16)=2 
      PX=PX-2
      ISOUND=1
      SPRITE 32,PX,PY,32
      IF CELL.C((PX+14)/16,PY/16)=6 THEN GOTO WIN
      WAIT VBL
    WEND
    IF CELL.C((PX+14)/16,PY/16)=8 THEN GOSUB PORTAL
    IF CELL.C((PX+14)/16,PY/16)=10 THEN GOSUB SLIDEUP
    IF CELL.C((PX+14)/16,PY/16)=12 THEN GOSUB SLIDEDOWN
    IF ISOUND=1 THEN PLAY 0,50,10 SOUND 5
    IF ISOUND=2 THEN PLAY 0,50,20 SOUND 1
    GOTO SKIP
  END IF
  
  IF RIGHT(0) THEN 
    ISOUND=0
    WHILE PX<144 AND NOT CELL.C((PX+16)/16,PY/16)=2 
      PX=PX+2
      ISOUND=1
      SPRITE 32,PX,PY,32
      IF CELL.C(PX/16,PY/16)=6 THEN GOTO WIN
      WAIT VBL
    WEND
    IF CELL.C(PX/16,PY/16)=8 THEN GOSUB PORTAL
    IF CELL.C(PX/16,PY/16)=10 THEN GOSUB SLIDEUP
    IF CELL.C(PX/16,PY/16)=12 THEN GOSUB SLIDEDOWN
    IF ISOUND=1 THEN PLAY 0,50,10 SOUND 5
    IF ISOUND=2 THEN PLAY 0,50,20 SOUND 1
    GOTO SKIP
  END IF
  
  IF CELL.C(PX/16,PY/16)=6 THEN GOTO WIN
  WAIT VBL
LOOP

PORTAL:
ISOUND=2
FOR Y=0 TO 7
  FOR X=0 TO 9
    IF CELL.C(X,Y)=8 THEN
      PX=X*16
      PY=Y*16
    END IF
  NEXT X
NEXT Y
RETURN

SLIDEUP:
PY=PY-32
RETURN

SLIDEDOWN:
PY=PY+32
RETURN

WIN:
PLAY 0,50,20 SOUND 2
WAIT 60
PX=16
PY=16
LVL=LVL+8
BG COPY 0,LVL,32,16 TO 0,0
IF LVL>64 THEN GOTO THEEND
GOTO RESTART

THEEND:
DISPLAY (0,1,1,0,0)
ATTR (4)
PRINT "MORE LEVELS COMING"
PRINT "SOON!"
END

