'*************************
'RECREATING POKEMON RED
'(C) NINTENDO 1995
'(C) GAME FREAK 1995
'(C) CREATURES INC. 1995
'DEVELOPMENT STARTED 2019
'*************************


'SOURCE OF MAP
BG SOURCE ROM(3)

'INITIALIZE ATTRIBUTES AND START THE MUSIC
GAMEPAD 1
SPRITE 0 SIZE 1
BG VIEW ON 0
MUSIC
CLS

'SET VARIABLES TO ELIMINATE ERRORS
MAP=0
MAPX=0
MAPY=0
DIR=0
M4=1
M5=1
STEPS=0
COOLDOWN=0
AN=0
BX=16
BY=32
GOX=16
GOY=32
MOVING=0

DO
'IF MAP=0 AND BX>95 AND BX<113 AND BY=-32 THEN
'BG SOURCE ROM(6)
'MAP=1
'END IF

'TRACE X AND Y POSITION
TRACE "X:",BX
TRACE "Y:",BY

'SCROLL MAP ACCORDINGLY TO CHARACTER POSITION, TO ELIMINATE 32X32 LIMIT
IF BX>=16 THEN MAPX=(BX/8)-2
IF BY>=16 THEN MAPY=(BY/8)-2
BG COPY MAPX,MAPY,32,32 TO MAPX,MAPY

'READ PLAYER INPUT
IF LEFT(0) THEN
IF CELL.C(INT(BX/8)+7,INT(BY/8)+8) = 0 OR CELL.C(INT(BX/8)+7,INT(BY/8)+8) = 150 OR CELL.C(INT(BX/8)+7,INT(BY/8)+8) = 137 OR CELL.C(INT(BX/8)+7,INT(BY/8)+8) = 139 THEN
GOX=GOX-16
MOVING=1
END IF
DIR=1
ELSE IF RIGHT(0) THEN
IF CELL.C(INT(BX/8)+11,INT(BY/8)+8) = 0 OR CELL.C(INT(BX/8)+11,INT(BY/8)+8) = 150 OR CELL.C(INT(BX/8)+11,INT(BY/8)+8) = 137 OR CELL.C(INT(BX/8)+11,INT(BY/8)+8) = 139 THEN
GOX=GOX+16
MOVING=1
END IF
DIR=2
ELSE IF UP(0) THEN
IF CELL.C(INT(BX/8)+9,INT(BY/8)+6) = 0 OR CELL.C(INT(BX/8)+9,INT(BY/8)+6) = 150 OR CELL.C(INT(BX/8)+9,INT(BY/8)+6) = 137 OR CELL.C(INT(BX/8)+9,INT(BY/8)+6) = 139 THEN
GOY=GOY-16
MOVING=1
END IF
DIR=3
ELSE IF DOWN(0) THEN
IF CELL.C(INT(BX/8)+9,INT(BY/8)+9) = 0 OR CELL.C(INT(BX/8)+9,INT(BY/8)+9) = 150 OR CELL.C(INT(BX/8)+9,INT(BY/8)+9) = 137 OR CELL.C(INT(BX/8)+9,INT(BY/8)+9) = 139 THEN
GOY=GOY+16
MOVING=1
END IF
DIR=0
END IF


'SCROLL SCREEN AND HANDLE ANIMS IF PLAYER MOVED
REPEAT
IF MOVING=1 THEN
IF M4=0 AND BX/8=INT(BX/8) THEN M4=1 ELSE IF M4=1 AND BX/8=INT(BX/8) THEN M4=0
IF M5=0 AND BY/8=INT(BY/8) THEN M5=1 ELSE IF M5=1 AND BY/8=INT(BY/8) THEN M5=0
ELSE
M4=0
END IF

IF DIR=0 AND M5=0 THEN
SPRITE 0,64,56,66
END IF
IF DIR=0 AND M5=1 AND STEPS=0 THEN
SPRITE 0,64,56,64
SPRITE 0 FLIP 0,0
END IF
IF DIR=0 AND M5=1 AND STEPS=1 THEN
SPRITE 0,64,56,64
SPRITE 0 FLIP 1,0
END IF

IF DIR=1 AND M4=0 THEN
SPRITE 0,64,56,68
SPRITE 0 FLIP 0,0
END IF
IF DIR=1 AND M4=1 THEN
SPRITE 0,64,56,70
SPRITE 0 FLIP 0,0
END IF

IF DIR=2 AND M4=0 THEN
SPRITE 0,64,56,68
SPRITE 0 FLIP 1,0
END IF
IF DIR=2 AND M4=1 THEN
SPRITE 0,64,56,70
SPRITE 0 FLIP 1,0
END IF

IF DIR=3 AND M5=0 THEN SPRITE 0,64,56,74
IF DIR=3 AND M5=1 AND STEPS=0 THEN
SPRITE 0,64,56,72
SPRITE 0 FLIP 0,0
END IF
IF DIR=3 AND M5=1 AND STEPS=1 THEN
SPRITE 0,64,56,72
SPRITE 0 FLIP 1,0
END IF

SCROLL 0,BX,BY
IF GOX<BX THEN BX=BX-1
IF GOX>BX THEN BX=BX+1
IF GOY<BY THEN BY=BY-1
IF GOY>BY THEN BY=BY+1
WAIT 1
UNTIL BX=GOX AND BY=GOY
MOVING=0
IF STEPS=1 THEN STEPS=0 ELSE STEPS=1
WAIT VBL
LOOP



