' JE TE L'AI MIS EN SUB POUR TE MONTRER 
' COMMENT CA MARCHE
SUB DRAWHOLE(X0, Y0, DX, DY)
  PRIO 0
  PAL 6
  FOR J=Y0 TO Y0+DY
    FOR I=X0 TO X0+DX
      CELL I, 10-J, 3
    NEXT I
  NEXT J
END SUB 

