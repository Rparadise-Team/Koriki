    SCROLL 0, 0, Y
    PLAY 0, NOTE,1 SOUND 3
    WAIT VBL
    ADD Y, -1
    DEC NOTE
  WEND
  
  SPRITE 1 SIZE 1
  SPRITE 1 PAL 3
  SPRITE 2 SIZE 1
  SPRITE 2 FLIP 1,0
  SPRITE 2 PAL 3
  X=0
  T=0
  
  'KINDA FAKE SCORE ATM
  SCORE=KM*100+MYMONEY*12+SWORD*24
  
  MUSIC
  
  DO
  
  IF T=0 THEN
  

  TEXT 4,3,"PRESS(START)"
  TEXT 4,1,"SCORE:"
  NUMBER 10,1,SCORE,6
  SPRITE 1,8,11.5*8,72
  SPRITE 2,17*8,11.5*8,72
  END IF
  
  IF T=30 THEN
  TEXT 4,3,"             "
  TEXT 4,1,"                      "
  SPRITE 1,8,11.5*8,74
  SPRITE 2,17*8,11.5*8,74
  END IF
  
  INC T
  IF T>60 THEN T=0
  
  IF PAUSE THEN 
  STOP
  TRACK 19, 0
  GOTO INIT
  END IF
  
  WAIT VBL
  LOOP

