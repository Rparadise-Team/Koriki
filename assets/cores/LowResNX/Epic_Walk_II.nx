    ADD XBOX, DX
    ADD YBOX, DY
    GOSUB CHECKBOX
  WEND
  'PREVENT THE BOX FOR GOING OVER THE MENU
  IF YBOX>73 THEN YBOX=73

  ' CHECK IF THE BOX SHOULD FALL
  IF BOXHASFALLEN=0 THEN 
    X=XBOX
    Y=YBOX
    GOSUB CHECKFALL
    NOTE=36
    IF FALLING = 1 THEN
      FOR I=0 TO 1
        PLAY 0, NOTE,1 SOUND 3
        DEC NOTE
        WAIT 5
      NEXT I
        
      BOXHASFALLEN=1
      BOXDOWN=2
    END IF 
  END IF

  ' CHANGING BOX ROOM
  IF XBOX<0 THEN 
    BOXROOM = BOXROOM-1
    XBOX = 144
  END IF
  IF XBOX>144 THEN
      BOXROOM = BOXROOM+1
      XBOX = 0
  END IF
  
RETURN 

